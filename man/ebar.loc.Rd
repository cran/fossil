\name{ebar.loc}
\docType{data}
\alias{ebar.loc}
\title{Early Barstovian Locality Data}
\description{
  This data set provides name, location and stratigraphic data for all the Early Barstovian localities from the Miocene Mammal Mapping Project (MIOMAP).
}
\usage{ebar.loc}
\format{A data frame containing 310 localities (rows) with data in 52 different fields (columns).}
\source{MIOMAP project}
\references{
Carrasco, M., Kraatz, B. Davis, E. and Barnosky., A. (2005) \emph{Miocene Mammal Mapping Project (MIOMAP)}. University of California Museum of Paleontology.

Carrasco, M., Barnosky, A., Kraatz, B. and Davis, E. (2007) \emph{The Miocene Mammal Mapping Project (MIOMAP): An Online Database of Arikareean Through Hemphillian Fossil Mammals}. Bulletin of Carnegie Museum of Natural History, 39: 183--188
}
\keyword{datasets}

